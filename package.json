{
  "name": "@zthun/identifier.core",
  "version": "2.0.1",
  "description": "The core library for the zidentifier feature.",
  "keywords": [
    "zid",
    "zidentifier",
    "zthunworks"
  ],
  "author": "Anthony Bonta",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/zidentifier.core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist *.tgz",
    "test": "jest --coverage",
    "lint": "tslint src/**/*.ts",
    "transpile": "tsc",
    "make": "npm run clean && npm run lint && npm run test && npm run transpile && npm pack"
  },
  "devDependencies": {
    "@types/jasmine": "=2.8.9",
    "@types/jest": "=23.3.5",
    "@types/jsdom": "^12.2.0",
    "@types/node": "=10.12.0",
    "@zthun/web-styles": "=6.0.0",
    "jasmine-core": "=3.2.1",
    "jest": "=23.6.0",
    "jsdom": "=12.2.0",
    "rimraf": "=2.6.2",
    "ts-jest": "=23.10.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.spec.json"
      }
    },
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../dist/coverage",
    "testEnvironment": "node"
  }
}
